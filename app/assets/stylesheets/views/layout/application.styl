body >
  
  #background
    dis blo
    bac img url('/images/home_guard_parade.jpg') siz cvr pos cen att fix

  #main
    bac clr hsla(0,0,100,0.5)

    > .menu
      bac clr hsla(0,0,90,0.9)
      txt siz 16 hei 30 ali cen
      sha 0 2 4 hsla(0,0,0,0.5)

      > .td
        wid 100

        &:hover
          bac clr hsl(0,0,85)

        &:active
          bac clr hsl(0,0,80)

        +on(1)
          bac clr hsl(0,0,80)

    > .yield
      pos rel
